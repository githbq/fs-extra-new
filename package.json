{
    "name": "fs-extra-new",
    "version": "1.0.0",
    "description": "fs-extra-new",
    "main": "build/index.js",
    "scripts": {
        "start:build": "npm run tsc && node build",
        "start": "node run",
        "test": "jest --forceExit --detectOpenHandles",
        "clear": "rimraf build",
        "tsc": "npm run clear && tsc",
        "tsc:w": "npm run tsc -- -w",
        "p": "npm run tsc && npm publish",
        "lint": "tslint \"src/**/*.ts\" --fix",
        "precommit": "npm run lint"
    },
    "dependencies": {
        "fs-extra": "^9.0.1",
        "tslib": "^1.13.0"
    },
    "devDependencies": {
        "@types/fs-extra": "^9.0.1",
        "@types/node": "^12.12.53",
        "husky": "^4.2.3",
        "jest": "^25.5.4",
        "rimraf": "^2.7.1",
        "ts-node": "^8.10.2",
        "tslint": "^6.1.2",
        "typescript": "^3.9.7"
    },
    "keywords": [
        "fs",
        "fs-extra",
        "fs-extra-promise",
        "isDirectory",
        "isDirectorySync",
        "fs-extra-new"
    ],
    "husky": {
        "hooks": {"pre-commit": "npm run lint"}
    },
    "author": "hubiqing",
    "repository": "https://github.com/githbq/hbq-fs-extra-new.git",
    "engines": {"node": ">=8"},
    "license": "MIT"
}